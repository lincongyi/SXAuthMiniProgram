.copyright {
  width: 100%;
  text-align: center;
  padding-bottom: 30rpx;
  box-sizing: border-box;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 1;
}

.copyright.isFixed {
  bottom: calc(env(safe-area-inset-bottom) + 16rpx + 110rpx);
}

.copyright .image-item {
  width: 200rpx;
  height: 48rpx;
}
